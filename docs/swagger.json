        "/api/v1/family/create": {
            "post": {
                "description": "创建一个新的家庭",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "family"
                ],
                "summary": "创建家庭",
                "parameters": [
                    {
                        "type": "string",
                        "description": "家庭名称",
                        "name": "name",
