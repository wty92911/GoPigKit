                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/api/v1/family/details": {
            "get": {
                "description": "获取指定家庭的详细信息，包括可选的预加载项",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "family"
                ],
                "summary": "获得家庭详细情况",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "家庭ID",
                        "name": "id",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "collectionFormat": "csv",
                        "description": "预加载项",
                        "name": "preloads",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/github_com_wty92911_GoPigKit_internal_model.Family"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
